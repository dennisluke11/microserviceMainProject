#mysql database configuration
spring.datasource.url=jdbc:mysql://localhost:3306/service?useUnicode=yes&characterEncoding=UTF-8&characterSetResults=UTF-8
spring.datasource.username=root
spring.datasource.password=root
spring.datasource.driver-class-name=com.mysql.cj.jdbc.Driver
spring.datasource.dialect=org.hibernate.dialect.MySQL5Dialect
spring.jpa.generate-ddl=true
spring.jpa.hibernate.ddl-auto = update
spring.jpa.hibernate.name.physical-strategy = org.hibernate.boot.model.naming.PhysicalNamingStrategyStandardImpl
message=message from student-service