#microsoft- mssql database configuration
datasource.url=jdbc:mysql://localhost:3306/service?useUnicode=yes&characterEncoding=UTF-8&characterSetResults=UTF-8

datasource.username=root
datasource.password=root
datasource.dialect=org.hibernate.dialect.MySQL5Dialect


message=default message from course-service

